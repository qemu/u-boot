#include "rk356x-u-boot.dtsi"

/ {
	chosen {
		stdout-path = &uart2;
		u-boot,spl-boot-order = "same-as-spl", &sdmmc0;
	};
};

&sdmmc0 {
	status = "okay";
};

&uart2 {
	clock-frequency = <24000000>;
	bootph-all;
	status = "okay";
};
