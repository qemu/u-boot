#include "rk3588-u-boot.dtsi"

/ {
	aliases {
		mmc0 = &sdmmc;
	};

	chosen {
		stdout-path = &uart2;
		u-boot,spl-boot-order = &sdmmc;
	};
};

&sdmmc {
	bus-width = <4>;
	u-boot,dm-pre-reloc;
	u-boot,spl-fifo-mode;
	status = "okay";
};
