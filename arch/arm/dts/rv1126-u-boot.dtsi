#include "rockchip-u-boot.dtsi"

/ {
	chosen {
		u-boot,spl-boot-order = \
			"same-as-spl", &emmc, &sdmmc;
	};

	dmc {
		compatible = "rockchip,rv1126-dmc";
		bootph-all;
	};
};

&gpio0 {
	bootph-pre-ram;
};

&gpio1 {
	bootph-pre-ram;
};

&grf {
	bootph-all;
};

&pmu {
	bootph-all;
};

&pmugrf {
	bootph-all;
};

&xin24m {
	bootph-pre-ram;
};

&cru {
	bootph-pre-ram;
};

&pmucru {
	bootph-pre-ram;
};

&sdmmc {
	bootph-pre-ram;
};

&emmc {
	bootph-pre-ram;
};

&uart2 {
	bootph-pre-ram;
};
